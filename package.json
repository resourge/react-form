{
  "name": "@resourge/react-form",
  "version": "1.22.0",
  "description": "react-form is a simple and basic controlled hook form. Aiming to create forms with minimal effort.",
  "main": "./dist/main.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "unpkg": "./dist/umd/react-form.production.min.js",
  "private": false,
  "type": "module",
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "react",
    "hooks",
    "form",
    "forms",
    "form-validation",
    "validation",
    "typescript",
    "react-hooks"
  ],
  "files": [
    "dist"
  ],
  "author": "resourge",
  "license": "MIT",
  "scripts": {
    "commitizen": "git-cz",
    "commit": "git pull && git add . && npm run commitizen",
    "lint": "eslint \"./src/**/*.{ts,tsx}\"",
    "lint:prod": "cross-env NODE_ENV=production npm run lint",
    "fix-types": "ts-node-esm ./scripts/FixTypes.ts",
    "dev": "vite",
    "build": "rollup -c && npm run fix-types",
    "test": "vitest run",
    "test:watch": "vitest",
    "coverage": "vitest run --coverage",
    "semantic-release": "semantic-release"
  },
  "devDependencies": {
    "@babel/core": "^7.21.0",
    "@babel/preset-env": "^7.20.2",
    "@babel/preset-react": "^7.18.6",
    "@babel/preset-typescript": "^7.21.0",
    "@rollup/plugin-babel": "^6.0.3",
    "@rollup/plugin-node-resolve": "^15.0.1",
    "@rollup/plugin-replace": "^5.0.2",
    "@rollup/plugin-terser": "^0.4.0",
    "@semantic-release/changelog": "^6.0.2",
    "@semantic-release/exec": "^6.0.3",
    "@semantic-release/git": "^10.0.1",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^14.0.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/react": "^18.0.28",
    "@types/react-dom": "^18.0.11",
    "@typescript-eslint/eslint-plugin": "^5.53.0",
    "@vitejs/plugin-react": "^3.1.0",
    "babel-plugin-dev-expression": "^0.2.3",
    "c8": "^7.13.0",
    "cross-env": "^7.0.3",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^8.34.0",
    "eslint-config-react-app": "^7.0.1",
    "eslint-config-standard-with-typescript": "^34.0.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-import-newlines": "^1.3.1",
    "eslint-plugin-n": "^15.6.1",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-testing-library": "^5.10.2",
    "eslint-plugin-typescript-sort-keys": "^2.1.0",
    "jsdom": "^21.1.0",
    "on-change": "^4.0.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "rollup": "^3.17.2",
    "rollup-plugin-dts": "^5.2.0",
    "rollup-plugin-filesize": "^9.1.2",
    "semantic-release": "^20.1.0",
    "typescript": "^4.9.5",
    "vite": "^4.1.4",
    "vite-plugin-checker": "^0.5.6",
    "vite-tsconfig-paths": "^4.0.5",
    "vitest": "^0.28.5"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/resourge/react-form.git"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "peerDependencies": {
    "react": ">=17.0.0"
  },
  "dependencies": {
    "@resourge/shallow-clone": "^1.0.0"
  }
}
