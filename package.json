{
  "name": "@resourge/react-form",
  "version": "1.16.0",
  "description": "react-form is a simple and basic controlled hook form. Aiming to create forms with minimal effort.",
  "main": "./dist/main.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "unpkg": "./dist/umd/react-form.production.min.js",
  "private": false,
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "react",
    "hooks",
    "form",
    "forms",
    "form-validation",
    "validation",
    "typescript",
    "react-hooks"
  ],
  "files": [
    "dist"
  ],
  "author": "resourge",
  "license": "MIT",
  "scripts": {
    "commitizen": "git-cz",
    "commit": "git pull && git add . && npm run commitizen",
    "lint": "eslint \"./src/**/*.{ts,tsx}\"",
    "lint:prod": "cross-env NODE_ENV=production npm run lint",
    "fix-types": "node ./scripts/FixTypes.js",
    "dev": "vite",
    "build": "rollup -c && npm run fix-types",
    "test": "vitest run",
    "test:watch": "vitest",
    "coverage": "vitest run --coverage",
    "semantic-release": "semantic-release"
  },
  "devDependencies": {
    "@babel/core": "^7.19.1",
    "@babel/preset-env": "^7.19.1",
    "@babel/preset-react": "^7.18.6",
    "@babel/preset-typescript": "^7.18.6",
    "@rollup/plugin-babel": "^5.3.1",
    "@rollup/plugin-node-resolve": "^14.1.0",
    "@rollup/plugin-replace": "^4.0.0",
    "@semantic-release/changelog": "^6.0.1",
    "@semantic-release/exec": "^6.0.3",
    "@semantic-release/git": "^10.0.1",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/react": "^18.0.20",
    "@types/react-dom": "^18.0.6",
    "@typescript-eslint/eslint-plugin": "^5.38.0",
    "@vitejs/plugin-react": "^2.1.0",
    "babel-plugin-dev-expression": "^0.2.3",
    "c8": "^7.11.0",
    "cross-env": "^7.0.3",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^8.23.1",
    "eslint-config-react-app": "^7.0.1",
    "eslint-config-standard-with-typescript": "^23.0.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-import-newlines": "^1.2.3",
    "eslint-plugin-n": "^15.3.0",
    "eslint-plugin-promise": "^6.0.1",
    "eslint-plugin-react": "^7.31.8",
    "eslint-plugin-testing-library": "^5.6.4",
    "eslint-plugin-typescript-sort-keys": "^2.1.0",
    "jsdom": "^20.0.0",
    "on-change": "^4.0.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "rollup": "^2.79.1",
    "rollup-plugin-dts": "^4.2.2",
    "rollup-plugin-filesize": "^9.1.2",
    "rollup-plugin-terser": "^7.0.2",
    "semantic-release": "^19.0.5",
    "typescript": "^4.8.3",
    "vite": "^3.1.3",
    "vite-plugin-checker": "^0.5.1",
    "vite-tsconfig-paths": "^3.5.0",
    "vitest": "^0.23.4"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/resourge/react-form.git"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "peerDependencies": {
    "react": ">=17.0.0"
  },
  "dependencies": {
    "@resourge/shallow-clone": "^1.0.0"
  }
}
