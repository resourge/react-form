{
  "name": "@resourge/react-form",
  "version": "1.2.0",
  "main": "./dist/main.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "unpkg": "./dist/umd/react-form.production.min.js",
  "private": false,
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "react",
    "hooks",
    "form",
    "forms",
    "form-validation",
    "validation",
    "typescript",
    "react-hooks"
  ],
  "files": [
    "dist"
  ],
  "author": "resourge",
  "license": "MIT",
  "scripts": {
    "commit": "git pull && git add . && git-cz",
    "lint": "eslint \"./src/**/*.{ts,tsx}\"",
    "lint:prod": "cross-env NODE_ENV=production npm run lint",
    "fix-types": "replace 'declare' 'export declare' dist/index.d.ts --silent",
    "dev": "vite",
    "build": "rollup -c && npm run fix-types",
    "test": "vitest run",
    "test:watch": "vitest",
    "coverage": "vitest run --coverage",
    "semantic-release": "semantic-release"
  },
  "peerDependencies": {
    "react": "^16.8.0 || ^17 || ^18"
  },
  "devDependencies": {
    "@babel/core": "^7.17.9",
    "@babel/preset-env": "^7.16.11",
    "@babel/preset-react": "^7.16.7",
    "@babel/preset-typescript": "^7.16.7",
    "@rollup/plugin-babel": "^5.3.1",
    "@rollup/plugin-commonjs": "^21.1.0",
    "@rollup/plugin-node-resolve": "^13.2.1",
    "@rollup/plugin-replace": "^4.0.0",
    "@semantic-release/changelog": "^6.0.1",
    "@semantic-release/exec": "^6.0.3",
    "@semantic-release/git": "^10.0.1",
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/react": "^13.1.1",
    "@testing-library/user-event": "^14.1.1",
    "@types/react": "^18.0.0",
    "@types/react-dom": "^18.0.0",
    "@typescript-eslint/eslint-plugin": "^4.33.0",
    "@typescript-eslint/parser": "^4.33.0",
    "@vitejs/plugin-react": "^1.3.0",
    "babel-eslint": "^10.1.0",
    "babel-plugin-dev-expression": "^0.2.3",
    "c8": "^7.11.0",
    "cross-env": "^7.0.3",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^7.32.0",
    "eslint-config-react-app": "^6.0.0",
    "eslint-config-standard-with-typescript": "^21.0.1",
    "eslint-plugin-flowtype": "^5.10.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.3.1",
    "eslint-plugin-react": "^7.29.4",
    "eslint-plugin-react-hooks": "^4.4.0",
    "eslint-plugin-testing-library": "^3.9.0",
    "jsdom": "^19.0.0",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "replace": "^1.2.1",
    "rollup": "^2.70.2",
    "rollup-plugin-cleaner": "^1.0.0",
    "rollup-plugin-dts": "^4.2.1",
    "rollup-plugin-size": "^0.2.2",
    "rollup-plugin-terser": "^7.0.2",
    "semantic-release": "^19.0.2",
    "typescript": "^4.6.3",
    "vite": "^2.9.2",
    "vite-plugin-checker": "^0.4.6",
    "vite-tsconfig-paths": "^3.4.1",
    "vitest": "^0.9.3"
  },
  "dependencies": {
    "on-change": "^4.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/resourge/react-form.git"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
